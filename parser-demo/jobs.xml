				<group>wsGroupTrigger</group>
				<job-name>ws</job-name>
				<job-group>wsGroup</job-group>
				<!--14:30 every day  -->
				<cron-expression>0 30 14 * * ?</cron-expression>
			</cron>
		</trigger>
		<trigger>
			<cron>
				<name>ws5</name>
				<group>wsGroupTrigger</group>
				<job-name>ws</job-name>
				<job-group>wsGroup</job-group>
				<!--19:30 every day  -->
				<cron-expression>0 30 19 * * ?</cron-expression>
			</cron>
		</trigger>
		<trigger>
			<cron>
				<name>ws6</name>
				<group>wsGroupTrigger</group>
				<job-name>ws</job-name>
				<job-group>wsGroup</job-group>
				<!--22:00 every day  -->
